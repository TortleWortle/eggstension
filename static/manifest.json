{
  "name": "Eggh Together",
  "version": "0.1",
  "description": "Fibsh",
  "browser_action": {
    "default_title": "Hi",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabCapture",
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "32": "images/icon@0,12x.png",
    "64": "images/icon@0,25x.png",
    "128": "images/icon@0,5x.png",
    "256": "images/icon.png"
  },
  "manifest_version": 2
}